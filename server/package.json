{
  "name": "server",
  "version": "1.0.0",
  "main": "app.js",
  "scripts": {
    "prestart": "npx sequelize db:migrate",
    "start": "node ./bin/www",
    "dev": "node --watch ./bin/www",
    "pretest": "cross-env NODE_ENV=test npx sequelize db:create --env test || echo 'DB already exists'",
    "test": "cross-env NODE_ENV=test jest --detectOpenHandles --forceExit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "pg": "^8.16.3",
    "sequelize": "^6.37.7"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "jest": "^30.2.0",
    "sequelize-cli": "^6.6.3",
    "supertest": "^7.1.4"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "testMatch": [
      "**/__test/**/*.test.js"
    ]
  }
}
